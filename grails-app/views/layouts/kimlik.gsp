<script>
    function masonryGallery()
    {
        var $container = $('.quicksand-gallery ul');
        $container.each(function()
        {
            var c = $(this);

            if (c.is('.masonry'))
                c.masonry('reload');

            c.imagesLoaded( function()
            {
                c.masonry({
                    gutterWidth: 18,
                    itemSelector : 'li',
                    columnWidth: c.find('li:first').width()-5,
                    isAnimated: true,
                    animationOptions: {
                        duration: 250,
                        easing: 'linear',
                        queue: true
                    }
                });
            });
        });
    }
    $(function()
    {
        masonryGallery();

        $(window).resize(function(e){
            masonryGallery();
        });
    });
</script>

